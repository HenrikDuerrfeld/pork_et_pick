 <h1> All bookings </h1>

 <ul>
  <% @bookings.each do |booking| %>
    <li><%= booking.user.first_name %></li>
    <li><%= booking.pig.name %></li>
    <li><%= booking.status ? "accepted" : "not accepted yet" %></li>
    <li><%= booking.start_date %></li>
    <li><%= booking.end_date %></li>
  <% end %>
 </ul>
